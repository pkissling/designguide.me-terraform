{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "acm:ListCertificates",
          "apigateway:*",
          "cloudFormation:*",
          "cloudFront:*",
          "cloudwatch:*",
          "events:*",
          "iam:*",
          "lambda:*",
          "logs:*",
          "route53:ListHostedZones",
          "route53:ChangeResourceRecordSets",
          "s3:*"
        ],
        "Resource": "*"
      }
    ]
  }